<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Complete Javadocs -->
  <suppress checks="MissingJavadocMethod" />

  <!-- Consistency with standard JDBC API names -->
  <suppress
    checks="ParameterName"
    files="[/\\]com[/\\]aoapps[/\\]sql[/\\]wrapper[/\\]"
    message="'(nClob|nString)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]sql[/\\]wrapper[/\\]"
    message="'new(NClob|SQLData|SQLInput|SQLOutput|SQLXML)Wrapper'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]sql[/\\]wrapper[/\\]"
    message="'(wrap|unwrap)(NClob|SQLData|SQLInput|SQLOutput|SQLXML)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]sql[/\\]wrapper[/\\](NClob|SQLData|SQLInput|SQLOutput|SQLXML)Wrapper\.java"
    message="'(NClob|SQLData|SQLInput|SQLOutput|SQLXML)Wrapper'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]sql[/\\]wrapper[/\\](NClob|SQLData|SQLInput|SQLOutput|SQLXML)WrapperImpl\.java"
    message="'(NClob|SQLData|SQLInput|SQLOutput|SQLXML)WrapperImpl'"
  />

  <!-- Will not reorder overloads -->
  <suppress checks="OverloadMethodsDeclarationOrder" />

</suppressions>
